      "command": "python",
      "args": [
        "c:\\Documents\\AI\\mcp仓库\\agents\\mcp-ssh\\bridging_ssh_mcp.py"
      ],
      "cwd": "c:\\Documents\\AI\\mcp仓库\\agents\\mcp-ssh",
      "env": {
        "SSH_TIMEOUT": "30",
        "SSH_CONFIG_PATH": "~/.ssh/config",
